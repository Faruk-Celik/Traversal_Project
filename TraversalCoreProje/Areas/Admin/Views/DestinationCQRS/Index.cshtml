@model List<GetAllDestinationQueryResult>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h1>CQRS TUR VERİLERİ</h1>

<br />
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>City</th>
        <th>Price</th>
        <th>Capacity</th>
        <th>Show in Site</th>
        <th>Delete</th>
        <th>Update</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.city</td>
            <td>@item.capacity</td>
            <td>@item.price ₺</td>
            <td>@item.daynight</td>
            <td><a href="/Admin/DestinationCQRS/DeleteDestination/@item.Id" class="btn btn-outline-danger">Delete</a></td>
            <td><a href="/Admin/DestinationCQRS/GetDestination/@item.Id" class="btn btn-outline-sucsess">Update</a></td>
        </tr>
    }
</table>
<a href="#" class="btn btn-outline-primary">Add Route</a>